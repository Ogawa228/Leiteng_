            rank_score = 0.2 *( 0.8 * ((combo_hits_ratio + 2 * k_dmg_damage_ratio) / 2) * 100
                          + 2 * (k_dmg_combo_ratio - received_k_dmg_combo_ratio)
                          + 2 * combo_count
                          + 0.01 * k_dmg
                          ) + 1.8 * len(hits)+ kill_death_ratio * 1 - tk_kills * 5